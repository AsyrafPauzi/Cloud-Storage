
<h2> People register : <%= @user.count %> </h2>
<h2> File uploads : <%= @file.count %> </h2>


<table class="table table-bordered table-striped" >
  <thead>
    <tr>
      <th>ID</th>
      <th>Email</th>
      <th>Sign In Count</th>
      <th>&nbsp;</th>
    </tr>
  </thead>
  <tbody>
    <% @user.each do |user| %>
      <tr align="center">
        <td><%= user.id %></td>
        <td><%= user.email %></td>
        <td><%= user.sign_in_count %></td>
        <td><%= link_to "Manage" , manage_path(user.id) %> </td>
      </tr>
    <% end %>
  </tbody>
</table>

